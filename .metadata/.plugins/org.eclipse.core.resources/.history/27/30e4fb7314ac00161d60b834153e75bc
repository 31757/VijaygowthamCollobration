package com.vanuchat.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.vanuchat.model.Job;

@Service
public class JobServiceImpl implements JobService{

	@Autowired
	JobDAO pDAO;
	
	public void insert(Job p) {
		// TODO Auto-generated method stub
		pDAO.insert(p);
	}

	public void update(Job p) {
		// TODO Auto-generated method stub
		pDAO.update(p);
	}

	public void delete(int pid) {
		// TODO Auto-generated method stub
		pDAO.delete(pid);
	}

	public Job getJob(int pid) {
		// TODO Auto-generated method stub
		return pDAO.getJob(pid);
	}

	public List<Job> ListJob() {
		// TODO Auto-generated method stub
		return pDAO.ListJob();
	}

	public Job getJobWithMaxId() {
		// TODO Auto-generated method stub
		return pDAO.getJobWithMaxId();
	}

}
